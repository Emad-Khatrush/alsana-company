<!DOCTYPE html>
<html>
  
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- reponsive meta-->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title><%= lang.homePage.home %></title>
    <!-- Bootstrap-->
    <link href="css/bootstrap.min.css" rel="stylesheet">
    <!-- animate css-->
    <link rel="stylesheet" href="css/animate.min.css">
    <link rel="stylesheet" href="css/font-awesome.min.css">
    <!-- owl-carousel-->
    <link rel="stylesheet" href="vendors/owlcarousel/owl.carousel.css">
    <link rel="stylesheet" href="vendors/revolution/css/settings.css">
    <link rel="stylesheet" href="vendors/revolution/css/layers.css">
    <link rel="stylesheet" href="vendors/revolution/css/navigation.css">
    <link rel="stylesheet" href="vendors/jquery-ui-1.11.4/jquery-ui.min.css">
    <!-- Main Css-->
    <link rel="stylesheet" href="css/style.css">
    <link rel="stylesheet" href="css/responsive.css">
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries-->
    <!-- WARNING: Respond.js doesn't work if you view the page via file://-->
    <!--if lt IE 9
    script(src='https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js')
    script(src='https://oss.maxcdn.com/respond/1.4.2/respond.min.js')
    -->
    <script src="https://kit.fontawesome.com/4163cc19b6.js" crossorigin="anonymous"></script>
  </head>
  <body>
    <!-- Top Navigation-->
    <div class="top-navigation-4">
      <!-- Header Gray Band-->
      <!-- Main Navigation-->
      <div class="main_menu menu_fixed nav-home-four">
        <div class="container">
          <div class="row">
            <div class="col-md-3 col-sm-12 col-xs-12">
              <div class="logo"><img src="assets/logo.png" alt=""></div>
            </div>
            <div class="col-md-9 col-sm-12 col-xs-12">
              <div class="nav-menu pull-right">
                <div class="nav-t-holder pull-right">
                  <div class="nav-t-header">
                    <button><i class="fas fa-bars"></i></button>
                  </div>
                  <div class="nav-t-footer">
                    <ul class="nav">
                      <li class=" active">
                          <a href="/<%= lang.currentLang %> "> <%= lang.homePage.home %> </a>
                      </li>
                      
                      <li class=""><a href="#projects"><%= lang.homePage.projects %></a></li>
                      <li><a href="/contact/<%= lang.currentLang %>"><%= lang.homePage.contactUs %></a></li>
                      <a></a>
                    </ul>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- Main Navigation-->
      <!-- =========home banner start============-->
      <div id="minimal-bootstrap-carousel" data-ride="carousel" class="carousel slide carousel-fade shop-slider full_width ver_new_4_slider">
        <!-- Wrapper for slides-->
        <div role="listbox" class="carousel-inner">
          <div style="background-image: url(assets/4-2.jpg);" class="item active">
            <div class="carousel-caption">
              <div class="thm-container txt-left">
                <div class="box valign-bottom">
                  <div class="content text-left">
                    <p data-animation="animated fadeInDown" style="font-size: 2.5rem; font-weight: bold;"><%= lang.homePage.alsanaSuccess %> </p>
                    <h1 data-animation="animated fadeInLeft" class="bnrfnt50"> <%= lang.homePage.technicians %> <%= lang.homePage.professional %> <span class="color3"><%= lang.homePage.certified %></span></h1>
                    <a data-animation="animated fadeInUp" href="#services" class="view-all hvr-bounce-to-right slide_learn_btn btn"><%= lang.homePage.ourServices %></a>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div style="background-image: url(assets/img/home1.jpg);" class="item">
            <div class="carousel-caption">
              <div class="thm-container">
                <div class="box valign-bottom">
                  <div class="content text-center">
                    <p data-animation="animated fadeInDown" style="font-size: 3rem; font-weight: bold;"><%= lang.homePage.alsanaSuccess %> </p>
                    <h1 data-animation="animated fadeInLeft" class="bnrfnt50"> <%= lang.homePage.leader %> <br><%= lang.homePage.manufacturing %> <span class="color3"><%= lang.homePage.buildings %></span></h1>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div style="background-image: url(assets/4-3.jpg);" class="item">
            <div class="carousel-caption">
              <div class="thm-container">
                <div class="box valign-bottom">
                  <div class="content text-center">
                    <p data-animation="animated fadeInDown" style="font-size: 3rem; font-weight: bold;"><%= lang.homePage.alsanaSuccess %> </p>
                    <h1 data-animation="animated fadeInLeft" class="bnrfnt50"><%= lang.homePage.projectsQuality %> <br> <span class="color3"><%= lang.homePage.hangers %></span> </h1>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- Controls--><a href="#minimal-bootstrap-carousel" role="button" data-slide="prev" class="left carousel-control"><i class="fas fa-arrow-left"></i><span class="sr-only">Previous</span></a><a href="#minimal-bootstrap-carousel" role="button" data-slide="next" class="right carousel-control"><i class="fas fa-arrow-right"></i>

<span class="sr-only">Next</span></a>
      </div>
      <!-- =========home banner end============-->
    </div>
    <!-- =========top navigation end============-->
    <ul class="banner_btm_list">
      <li class="lytblue_prd_bg"><img src="assets/icon-1.png" alt="">
        <h4 style="font-size: 2.5rem; font-weight: bold;"><%= lang.homePage.expertEngineers %> </h4>
        <p style="font-size: 1.7rem;" ><%= lang.homePage.expertEngineersDesc %></p>
      </li>
      <li class="blue_prd_bg"><img src="assets/icon-2.png" alt="">
        <h4 style="font-size: 2.5rem; font-weight: bold;"><%= lang.homePage.qualityservices %></h4>
        <p style="font-size: 1.7rem;" ><%= lang.homePage.qualityservicesDesc %></p>      </li>
      <li class="yellow_prd_bg"><img src="assets/icon-3.png" alt="">
        <h4 style="font-size: 2.5rem; font-weight: bold;"><%= lang.homePage.modernEquipments %></h4>
        <p style="font-size: 1.7rem; color: white;" ><%= lang.homePage.modernEquipmentsDesc %></p>      </li>
    </ul>
    <!-- Control in compliance-->
    <section class="container ind-common-pad2 clearfix">
      <div class="quality-wrapper text-center">
        <div class="vision tab-panel fade in">
          <h2><%= lang.homePage.qualityControlDesc %></h2>
        </div>
        <div class="values tab-panel fade hide">
          <h2><%= lang.homePage.qualityControlDesc %></h2>
        </div>
        <div class="mission tab-panel fade hide">
          <h2><%= lang.homePage.qualityControlDesc %></h2>
        </div>
        <div class="vision-wrapper text-center">
          <ul>
            <li><a id="vision" href="javascript:void(0)"><img src="assets/ser-icon4.png" alt="">
                <p> <%= lang.homePage.qualityControl %>  </p></a></li>
            <li><a id="values" href="javascript:void(0)"><img src="assets/ser-icon5.png" alt="">
                <p><%= lang.homePage.distinction %></p></a></li>
            <li><a id="mission" href="javascript:void(0)"><img src="assets/ser-icon6.png" alt="">
                <p><%= lang.homePage.missions %></p></a></li>
          </ul>
        </div>
      </div>
    </section>
    <!-- Control in compliance-->
    <!-- Team-->
    <section class="team-wrapper team-wrapper4" id="services">
      <div class="container clearfix">
        <div class="our-team-page">
          <div class="section_header2">
            <h2><%= lang.homePage.ourServices %></h2>
            <div class="decor-line"></div>
          </div>
          <div class="row event-pad">
            <div class="col-sm-6 col-md-4 team">
              <div class="box-img"><img src="assets/img/hangars.jpg" alt="">
                <div class="caption">
                  <h2><%= lang.homePage.hangersProduction %> </h2>
                  <p><%= lang.homePage.hangerDesc %> </p>
                </div>
              </div>
            </div>
            <div class="col-sm-6 col-md-4 team">
              <div class="box-img"><img src="assets/img/portable-tankers.jpg" alt="">
                <div class="caption">
                  <h2><%= lang.homePage.saharij %></h2>
                  <p><%= lang.homePage.saharijDesc %></p>
                  <br>
                </div>
              </div>
            </div>
            <div class="col-sm-6 col-md-4 team">
              <div class="box-img"><img src="assets/img/storage-tankers.jpg" alt="">
                <div class="caption">
                  <h2><%= lang.homePage.saharij2 %></h2>
                  <p style="font-size: 1.405rem;"><%= lang.homePage.saharij2Desc %></p>
                </div>
              </div>
            </div>
            <div class="col-sm-6 col-md-4 team">
              <div class="box-img"><img src="assets/img/containers.jpg" alt="">
                <div class="caption">
                  <h2><%= lang.homePage.campo %></h2>
                  <p style="font-size: 1.35rem;"><%= lang.homePage.campoDesc %></p>
                </div>
              </div>
            </div>
            <div class="col-sm-6 col-md-4 team">
              <div class="box-img"><img src="assets/img/beds.jpg" alt="">
                <div class="caption">
                  <h2><%= lang.homePage.beds %></h2>
                  <p><%= lang.homePage.bedsDesc %></p>
                </div>
              </div>
            </div>
            <div class="col-sm-6 col-md-4 team">
              <div class="box-img"><img src="assets/img/cupboards.jpg" alt="">
                <div class="caption">
                  <h2><%= lang.homePage.dolap %></h2>
                  <p><%= lang.homePage.dolapDesc %></p>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
    <!-- Our Projects-->
    <section class="our-galler-four clearfix" id="projects">
      <div class="container clearfix">
        <div class="section_header common">
          <h2 class="white"><%= lang.homePage.ourProjects %></h2>
        </div>
      </div>
      <div class="fullwidth-silder3">
        <div class="fullwidth-slider3">
          <% projects.forEach(project => { %>
              <div class="item project_img"><img src="<%= project.img[0] %>" alt="">
                <div class="project_txt_btn"><a href="/project/<%= project.id %>/<%= lang.currentLang %>" class="view-all desi-bounce-to-right slide_learn_btn view_project_btn"><%= lang.homePage.viewProject %></a>
                  <h6> <%= project.title %> </h6>
                </div>
              </div>
          <% });  %> 
        </div>
      </div>
    </section>
    <!-- Latest News-->
    <section class="latest-news1 sectpad about-us4">
      <div class="container clearfix">
        <div class="row">
          <div class="col-lg-6 about-sec-content">
            <div class="section_header2 common">
              <h2><%= lang.homePage.aboutUs %></h2>
            </div>
            <div class="row event-pad">
              <div class="col-md-6"><img src="assets/4-1.jpg" alt=""></div>
              <div class="col-md-6"><img src="assets/4-22.jpg" alt=""></div>
            </div>
            <h4><%= lang.homePage.alsanaDesc %></h4>
            <h4> <%= lang.homePage.alsanaDesc2 %></h4>
            <div class="row">
              <div class="col-md-12">
                <ul>
                  <li><i class="fa fa-arrow-circle-right"></i> <%= lang.homePage.service3 %></li>
                  <li><i class="fa fa-arrow-circle-right"></i> <%= lang.homePage.service4 %></li>
                </ul>
              </div>
              <div class="col-md-12">
                <ul>
                  <li><i class="fa fa-arrow-circle-right"></i> <%= lang.homePage.service5 %></li>
                  
                  <li><i class="fa fa-arrow-circle-right"></i> <%= lang.homePage.service2 %></li>
                </ul>
              </div>
              <div class="col-md-12">
                <ul>
                  <li><i class="fa fa-arrow-circle-right"></i> <%= lang.homePage.service1 %></li>
                  <li><i class="fa fa-arrow-circle-right"></i> <%= lang.homePage.service6 %></li>
                </ul>
              </div>
            </div>
          </div>
          <div class="col-lg-6">
            <div class="section-faq">
              <div class="section_header2 common">
                <h2><%= lang.homePage.frequentlyAskedQuestions %></h2>
              </div>
              <div class="accordian-area accordian-area-pad">
                <div id="accordion" role="tablist" aria-multiselectable="true" class="panel-group">
                  <div class="panel panel-default">
                    <div id="headingOne" role="tab" class="panel-heading">
                      <h4 class="panel-title"><a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="false" aria-controls="collapseOne" class="collapsed"><span><%= lang.homePage.question1 %></span><i class="fa fa-minus"></i><i class="fa fa-plus"></i></a></h4>
                    </div>
                    <div id="collapseOne" role="tabpanel" aria-labelledby="headingOne" class="panel-collapse collapse">
                      <div class="panel-body faq-content">
                        <p><%= lang.homePage.answer1 %></p>
                      </div>
                    </div>
                  </div>
                  <div class="panel panel-default">
                    <div id="headingTwo" role="tab" class="panel-heading">
                      <h4 class="panel-title on"><a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseTwo" aria-expanded="true" aria-controls="collapseTwo"><%= lang.homePage.question2 %><i class="fa fa-minus"></i><i class="fa fa-plus"></i></a></h4>
                    </div>
                    <div id="collapseTwo" role="tabpanel" aria-labelledby="headingTwo" class="panel-collapse collapse in">
                      <div class="panel-body faq-content">
                        <p><%= lang.homePage.answer2 %></p>
                      </div>
                    </div>
                  </div>
                  <div class="panel panel-default">
                    <div id="headingThree" role="tab" class="panel-heading">
                      <h4 class="panel-title"><a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseThree" aria-expanded="false" aria-controls="collapseThree" class="collapsed"><%= lang.homePage.question3 %><i class="fa fa-minus"></i><i class="fa fa-plus"></i></a></h4>
                    </div>
                    <div id="collapseThree" role="tabpanel" aria-labelledby="headingThree" class="panel-collapse collapse">
                      <div class="panel-body faq-content">
                        <p><%= lang.homePage.answer3 %></p>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>

    <!-- Indurial Solution-->
    <footer class="sec-padding footer-bg footer-bg3">
      <div class="container clearfix">
        <div class="row">
          <div class="widget about-us-widget col-md-4 col-sm-6"><a href="/<%= lang.currentLang %> "><img src="/assets/logo.png" alt=""></a>
          </div>
          <div class="widget widget-links col-md-4 col-sm-6">
            <h4 class="widget_title"><%= lang.homePage.quickLinks %> </h4>
            <div class="widget-contact-list row m0">
              <ul>
                <li><a href="/<%= lang.currentLang %>">- <%= lang.homePage.home %></a></li>
                <li><a href="/contact/<%= lang.currentLang %>">- <%= lang.homePage.contactUs %></a></li>
                <li><a href="https://www.facebook.com/assanea" target="_blank"><i class="fa fa-facebook-square"></i></a></li>
              </ul>
            </div>
          </div>
          <div class="widget widget-contact col-md-4 col-sm-6">
            <h4 class="widget_title"><%= lang.homePage.getInTouch %></h4>
            <div class="widget-contact-list row m0">
              <ul>
                <li><i class="fas fa-map-marker"></i>
                  <div class="fleft location_address" style="padding-top: 21px;"> <%= lang.contactPage.address %> - <%= lang.contactPage.city %></br> </br></div>
                </li>
                <li><i class="fas fa-phone"></i>
                  <div class="fleft contact_no"><a href="#"></a><%= lang.contactPage.phone %></div>
                </li>
                <li><i style="margin-top: 15px;" class="fas fa-envelope"></i>
                  <div class="fleft contact_mail"><a href="mailto:<%= lang.contactPage.alsanaEmail %>"><%= lang.contactPage.alsanaEmail %></a></div>
                </li>
                <li><i class="icon icon-Timer"></i>
                  <div class="fleft service_time" style="padding-top: 21px;"><%= lang.contactPage.worksTime %></div>
                </li>
              </ul>
            </div>
          </div>
        </div>
      </div>
    </footer>

    <script src="js/jquery-1.12.2.min.js"></script>
    <script src="js/bootstrap.min.js"></script>
    <!-- RS-->
    <script src="vendors/revolution/js/jquery.themepunch.tools.min.js"></script>
    <!-- Revolution Slider Tools-->
    <script src="vendors/revolution/js/jquery.themepunch.revolution.min.js"></script>
    <!-- Revolution Slider-->
    <script type="text/javascript" src="vendors/revolution/js/extensions/revolution.extension.slideanims.min.js"></script>
    <script type="text/javascript" src="vendors/revolution/js/extensions/revolution.extension.layeranimation.min.js"></script>
    <script type="text/javascript" src="vendors/revolution/js/extensions/revolution.extension.navigation.min.js"></script>
    <script src="js/jquery.form.js"></script>
    <script src="js/jquery.validate.min.js"></script>
    <script src="js/contact.js"></script>
    <!-- owl carousel-->
    <script src="vendors/owlcarousel/owl.carousel.min.js"></script>
    <script src="vendors/jquery-ui-1.11.4/jquery-ui.min.js"></script>
    <script src="js/search.js"></script>
    <script src="js/theme.js"></script>
  </body>

</html>